<div class="blue-header">
    <div class="blue-header-text">
        <div class="blue-header-title">
            <%= page.tag %>
        </div>
        <div>
            More posts related to <%= page.tag %>
        </div>
    </div>
</div>
<div class="container">
    <!-- Blog excerpts -->
    <div class="blog-main">
        <% page.posts.each(function(item){ %>
            <%- partial('_partial/article-excerpt', {item: item}) %>
        <% }); %>
        <%- partial('_partial/pagination') %>
    </div>
</div>
</div>